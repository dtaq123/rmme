<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.dtaq.myrmme.MainActivity"
    tools:showIn="@layout/app_bar_main">
    <!--android:paddingBottom="@dimen/activity_vertical_margin"-->
    <!--android:paddingLeft="@dimen/activity_horizontal_margin"-->
    <!--android:paddingRight="@dimen/activity_horizontal_margin"-->
    <!--android:paddingTop="@dimen/activity_vertical_margin"-->

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar2"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="50dp"
        android:src="@drawable/avatar_2" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@+id/avatar2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:src="@drawable/avatar_5" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:src="@drawable/avatar_1" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar3"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_above="@+id/avatar5"

        android:layout_toEndOf="@+id/avatar1"
        android:layout_toRightOf="@+id/avatar1"
        android:src="@drawable/avatar_3" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar4"
        android:layout_width="80dp"
        android:layout_height="80dp"

        android:layout_alignTop="@+id/avatar5"

        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_toLeftOf="@+id/avatar1"
        android:layout_toStartOf="@+id/avatar1"
        android:src="@drawable/avatar_4" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/avatar6"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/avatar3"
        android:layout_alignRight="@+id/avatar3"
        android:layout_alignTop="@+id/avatar5"
        android:src="@drawable/avatar_6" />


    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white_overlay">

        <com.getbase.floatingactionbutton.FloatingActionsMenu
            android:id="@+id/fab_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            app:fab_addButtonColorNormal="@color/colorAccent"
            app:fab_addButtonColorPressed="@color/colorPrimaryDark"
            app:fab_addButtonPlusIconColor="@color/white"
            app:fab_addButtonStrokeVisible="false"
            app:fab_labelStyle="@style/menu_labels_style"
            app:fab_labelsPosition="left">

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_event"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fab_colorNormal="@color/colorAccent"
                app:fab_colorPressed="@color/colorPrimaryDark"
                app:fab_icon="@drawable/ic_menu_setting"
                app:fab_plusIconColor="@color/white"
                app:fab_size="mini"
                app:fab_title="Setting" />

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_photo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fab_colorNormal="@color/colorAccent"
                app:fab_colorPressed="@color/colorPrimaryDark"
                app:fab_icon="@drawable/ic_menu_history"
                app:fab_plusIconColor="@color/white"
                app:fab_size="mini"
                app:fab_title="History" />

            <com.getbase.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_others"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:fab_colorNormal="@color/colorAccent"
                app:fab_colorPressed="@color/colorPrimaryDark"
                app:fab_icon="@drawable/ic_menu_home"
                app:fab_plusIconColor="@color/white"
                app:fab_size="mini"
                app:fab_title="Home" />


        </com.getbase.floatingactionbutton.FloatingActionsMenu>
    </FrameLayout>


</RelativeLayout>